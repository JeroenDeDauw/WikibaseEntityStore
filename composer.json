{
	"name": "ppp/wikibase-entity-store",
	"type": "library",
	"description": "A small library to store Wikibase entities",
	"keywords": [
		"wikidata",
		"wikibase"
	],
	"homepage": "https://github.com/ProjetPP/WikibaseEntityStore",
	"license": "GPLv2+",
	"authors": [
		{
			"name": "Thomas Pellissier Tanon",
			"homepage": "https://github.com/Tpt"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"wikibase/data-model": "~2.5",
		"wikibase/data-model-serialization": "~1.2", 
		"data-values/data-values": "~1.0|~0.1",
		"data-values/common": "~0.2.0",
		"data-values/geo": "~1.0",
		"data-values/number": "~0.4.0",
		"data-values/time": "~0.6.0",
		"data-values/validators": "~0.1.0",
		"data-values/data-types": "~0.4.0",
		"data-values/serialization": "~1.0",
		"addwiki/wikibase-api": "~0.4"
	},
	"require-dev": {
		"symfony/console": "~2.6"
	},
	"suggest": {
		"symfony/console": "Allows use of the command line interface"
	},
	"autoload": {
		"psr-4": {
			"Wikibase\\EntityStore\\": "src/"
		}
	}
}
